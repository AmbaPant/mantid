<?xml version="1.0" encoding="UTF-8"?>
<parameter-file instrument = "D11B">

    <component-link name="D11B">
      <!-- The name of the panels for separate I(Q) calculations, the names are from the IDF -->
      <parameter name="detector_panels" type="string">
        <value val="detector_center,detector_left,detector_right"/>
      </parameter>
      <!-- Tau coefficient for DeadTimeCorrection, to be applied tube by tube -->
      <parameter name="tau" type="number">
        <value val="0.0000015" />
      </parameter>
      <!-- Logs to sum when merging the runs -->
      <parameter name="sample_logs_sum" type="string">
        <value val="time, timer, duration" />
      </parameter>
      <!-- The pixel sizes [mm] used by SolidAngle, resolution and default Q binning calculations.
      Note that for the panels, often pixels have transposed shape, but the surface is the same.
      Q binning is based on the maximum of the 2 dimensions, so it is also invariant. -->
      <parameter name="x-pixel-size" type="number">
        <value val="4"/>
      </parameter>
      <parameter name="y-pixel-size" type="number">
        <value val="8"/>
      </parameter>
      <!-- These parameters are used to define headers and entries for GenerateLogbook algorithm -->
      <parameter name="logbook_default_headers" type="string">
	<value val="SampleDescription,TotalTime,RateMonitor1,TotalCountsDetMain,RateCountsDetMain,StartTime,Wavelength,Attenuator,Collimation,SD,BeamStopY" />
      </parameter>
      <parameter name="logbook_default_entries" type="string">
	<value val="/entry0/sample_description,/entry0/duration,/entry0/monitor1/monrate,/entry0/D11/detector/detsum,/entry0/D11/detector/detrate,/entry0/start_time,/entry0/D11/selector/wavelength,/entry0/D11/attenuator/attenuation_value,/entry0/D11/collimation/actual_position,/entry0/D11/detector/det_actual,/entry0/D11/beamstop/by_actual" />
      </parameter>
      <parameter name="logbook_optional_headers" type="string">
	<value val="AcquisitionMode,Beamstop,VelocitySelectorSpeed,VelocitySelectorTilt,AttenuationFactor,Diaphragm1,Diaphragm2,Diaphragm3,Diaphragm4,Diaphragm5,Diaphragm6,Guide1,Guide2,Guide3,Guide4,Guide5,Guide6,Guide7,Guide8,Guide9,Guide10,Guide11,Guide12,Guide13,TotalCountsMonitor1,ReactorPower,BeamstopX,BeamstopX2,TotalCountsDetLeft,TotalCountsDetRight,RateCountsDetLeft,RateCountsDetRight,san,omega,phi,sdi,sht,str,trs,SampleTemperature,AirTemperature,RackTemperature,Bath1Temperature,Bath2Temperature,BathSelector,Thermocouple1,Thermocouple2,SampleChanger,Position,SampleThickness,MeasurerementType,SampleType,SampleSubType,SampleApertureWidth,SampleApertureHeight" />
      </parameter>
      <parameter name="logbook_optional_entries" type="string">
	<value val="/entry0/acquisition_mode,/entry0/D11/beamstop/actual_beamstop_number,/entry0/D11/selector/rotation_speed,/entry0/D11/selector/selrot_actual,/entry0/D11/attenuator/attenuation_coefficient,/entry0/D11/collimation/diaphragm1_position,/entry0/D11/collimation/diaphragm2_position,/entry0/D11/collimation/diaphragm3_position,/entry0/D11/collimation/diaphragm4_position,/entry0/D11/collimation/diaphragm5_position,/entry0/D11/collimation/diaphragm6_position,/entry0/D11/collimation/coll1_actual_state,/entry0/D11/collimation/coll2_actual_state,/entry0/D11/collimation/coll3_actual_state,/entry0/D11/collimation/coll4_actual_state,/entry0/D11/collimation/coll5_actual_state,/entry0/D11/collimation/coll6_actual_state,/entry0/D11/collimation/coll7_actual_state,/entry0/D11/collimation/coll8_actual_state,/entry0/D11/collimation/coll9_actual_state,/entry0/D11/collimation/coll10_actual_state,/entry0/D11/collimation/coll11_actual_state,/entry0/D11/collimation/coll12_actual_state,/entry0/D11/collimation/coll13_actual_state,/entry0/monitor1/data,/entry0/reactor_power,/entry0/D11/beamstop/bx_actual,/entry0/D11/beamstop/bx2_actual,/entry0/D11/detectorLeft/detsum,/entry0/D11/detectorRight/detsum,/entry0/D11/detectorLeft/detrate,/entry0/D11/detectorRight/detrate,/entry0/sample/san_actual,/entry0/sample/omega_actual,/entry0/sample/phi_actual,/entry0/sample/sdi_actual,/entry0/sample/sht_actual,/entry0/sample/str_actual,/entry0/sample/trs_actual,/entry0/sample/temperature,/entry0/sample/air_temperature,/entry0/sample/rack_temperature,/entry0/sample/bath1_regulation_temperature,/entry0/sample/bath2_regulation_temperature,/entry0/sample/Actual bath,/entry0/sample/thermo_temperature1,/entry0/sample/thermo_temperature2,/entry0/sample/sample_changer_nickname,/entry0/sample/sample_changer_slot_value,/entry0/sample/thickness,/entry0/Measurement_type/typeOfMeasure,/entry0/Measurement_type/typeOfSample,/entry0/Measurement_type/subType,/entry0/D11/Beam/sample_ap_x_or_diam,/entry0/D11/Beam/sample_ap_y" />
      </parameter>
    </component-link>
    <!-- These parameters are used in ParallaxCorrection algorithm -->
    <component-link name="detector_center">
        <parameter name="parallax" type="string">
            <value val="1+0.14*exp(-4*ln(2.)*((t-0.588)/0.414)^2)"/>
        </parameter>
        <parameter name="direction" type="string">
            <value val="x"/>
        </parameter>
    </component-link>
    <component-link name="detector_left">
        <parameter name="parallax" type="string">
            <value val="1+0.14*exp(-4*ln(2.)*((t-0.588)/0.414)^2)"/>
        </parameter>
        <parameter name="direction" type="string">
            <value val="y"/>
        </parameter>
    </component-link>
    <component-link name="detector_right">
        <parameter name="parallax" type="string">
            <value val="1+0.14*exp(-4*ln(2.)*((t-0.588)/0.414)^2)"/>
        </parameter>
        <parameter name="direction" type="string">
            <value val="y"/>
        </parameter>
    </component-link>

</parameter-file>
