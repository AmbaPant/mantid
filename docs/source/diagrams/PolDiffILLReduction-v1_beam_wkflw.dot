digraph Beam {
  $global_style

  subgraph params {
    $param_style
    Run
    AbsorberTransmissionWorkspace
  }

  subgraph values {
    $value_style
    OutputWorkspace
  }

  subgraph algorithms {
    $algorithm_style
    LoadAndMerge
    Divide [label="Normalize to monitor/time"]
    Minus
  }

  Run -> LoadAndMerge
  LoadAndMerge -> Divide
  Divide -> Minus
  AbsorberTransmissionWorkspace -> Minus
  Minus -> OutputWorkspace
}
