digraph Vanadium {
  $global_style

  subgraph decisions {
    $decision_style
    ConvertUnits
    SumCountsOrAverage
  }

  subgraph params {
    $param_style
    Run
    D7YIGPositionCalibration
    AbsorberInputWorkspace
    ContainerInputWorkspace
    QuartzInputWorkspace
    TransmissionInputWorkspace
    PaalmanPingsMonteCarloAbsorption
    SampleAndEnvironmentProperties
  }

  subgraph values {
    $value_style
    OutputWorkspace
  }

  subgraph algorithms {
    $algorithm_style
    Load
    Divide
    Multiply
    Minus
    Plus
    Divide2 [label="Divide"]
    SumOverlappingTubes
    CrossSectionSeparation [label="Component separation"]
  }

  Run -> Load
  D7YIGPositionCalibration -> Load
  Load -> Divide
  Divide -> Minus
  AbsorberInputWorkspace -> Minus
  ContainerInputWorkspace -> Minus
  TransmissionInputWorkspace -> Minus
  Minus -> Multiply
  QuartzInputWorkspace -> Multiply
  Multiply -> PaalmanPingsMonteCarloAbsorption
  SampleAndEnvironmentProperties -> PaalmanPingsMonteCarloAbsorption
  PaalmanPingsMonteCarloAbsorption -> SumCountsOrAverage
  SumCountsOrAverage -> SumOverlappingTubes
  SumCountsOrAverage -> Plus
  SumOverlappingTubes -> CrossSectionSeparation
  Plus -> CrossSectionSeparation
  CrossSectionSeparation -> ConvertUnits
  ConvertUnits -> ConvertSpectrumAxis
  ConvertSpectrumAxis -> Divide2
  ConvertUnits -> Divide2
  Divide2 -> OutputWorkspace
}
