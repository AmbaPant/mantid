digraph Transmission {
  $global_style

  subgraph params {
    $param_style
    Run
    AbsorberTransmissionWorkspace
    EmptyBeamWorkspace
  }

  subgraph values {
    $value_style
    OutputWorkspace
  }

  subgraph algorithms {
    $algorithm_style
    LoadAndMerge
    Divide [label="Normalize to monitor/time"]
    Divide2 [label="Divide"]
    Minus
  }

  Run -> LoadAndMerge
  LoadAndMerge -> Divide
  Divide -> Minus
  AbsorberTransmissionWorkspace -> Minus
  EmptyBeamWorkspace -> Divide
  Minus -> Divide
  Divide -> OutputWorkspace
}
