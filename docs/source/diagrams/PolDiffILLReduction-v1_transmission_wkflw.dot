digraph Transmission {
  $global_style

  subgraph params {
    $param_style
    Run
    AbsorberTransmissionInputWorkspace
    BeamInputWorkspace
  }

  subgraph values {
    $value_style
    OutputWorkspace
  }

  subgraph algorithms {
    $algorithm_style
    Load
    Divide
    Minus
  }

  Run -> Load
  Load -> Minus
  AbsorberTransmissionInputWorkspace -> Minus
  Minus -> Divide
  BeamInputWorkspace -> Divide
  Divide -> OutputWorkspace
}
